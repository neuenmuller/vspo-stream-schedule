# Local Development with Firebase Emulator
# Copy this file to .env.local for local development

# Firebase Configuration (can be dummy values for emulator)
VITE_API_KEY=demo-api-key
VITE_AUTH_DOMAIN=demo-project.firebaseapp.com
VITE_PROJECT_ID=demo-project
VITE_STORAGE_BUCKET=demo-project.appspot.com
VITE_MESSAGING_SENDER_ID=123456789
VITE_DATABASE_URL=https://demo-project.firebaseio.com
VITE_APP_ID=1:123456789:web:abcdef

# Firebase Collections
VITE_STREAM_COLLECTION_NAME=streams
VITE_STREAMER_COLLECTION_NAME=streamers

# Enable Firebase Emulator
VITE_USE_EMULATOR=true
